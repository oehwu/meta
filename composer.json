{
    "name": "oehwu/meta",
    "description": "Package with various small handy classes in OEHWU projects",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Rieschl",
            "email": "thomas.rieschl@oeh-wu.at"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "OEHWU\\Meta\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OEHWUTest\\Meta\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
